function desktopInit()
{mCategoryGrover.off();mCategoryGrover.removeClass('active');$('.sf-menu > li > ul').removeClass('menu-mobile').parent().find('.menu-mobile-grover').remove();$('.sf-menu').removeAttr('style');categoryMenu.superfish('init');$('.sf-menu > li > ul').addClass('submenu-container clearfix');$('.sf-menu > li > ul.has-menu-banner').each(function(){var menubanner=$(this).find('.menu-banner-wrapper').getSize();$(this).css('min-height',menubanner.height+10);});$('.sf-menu > li > ul').each(function(){$(this).width($('.main-navigation .container').width());$(this).css('max-width',$('.main-navigation .container').width());i=0;$(this).each(function(){if($(this).attr('class')!="category-thumbnail"){i++;if(i%2==1)
$(this).addClass('first-in-line-xs');else if(i%5==1)
$(this).addClass('first-in-line-lg');}});});}
function mobileInit()
{$('.header-menu .first-list li').appendTo($('.dl-menu'));$('.header-menu .second-list li:first').appendTo($('.dl-menu'));$('.header-menu .second-list li').appendTo($('.mobile-header-nav'));$('.dl-menu li').addClass("momento");$('.menu-content .category-thumb').remove();$('.menu-content .menu-banner').remove();$('.menu-content > li').appendTo($('.dl-menu'));$('#block_top_menu').remove();$('.header-menu').remove();$('.todlmenu').appendTo($('#dl-menu'));$('.dl-menu li ul').addClass("dl-submenu");$('#dl-menu').dlmenu();}
