$(document).on('click','.product_quantity_up',function(e){e.preventDefault();fieldName=$(this).data('field-qty');var currentVal=parseInt($('input[name='+fieldName+']').val());if(!allowBuyWhenOutOfStock&&quantityAvailable>0)
